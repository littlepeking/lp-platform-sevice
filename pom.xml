<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<packaging>pom</packaging>
	<modules>
		<module>enhantec-framework-service</module>
		<module>enhantec-wms-service</module>
	</modules>
	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>2.6.5</version>
		<relativePath/> <!-- lookup parent from repository -->
	</parent>
	<groupId>com.enhantec</groupId>
	<artifactId>enhantec-framework</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>enhantec-framework</name>
	<description>Enhantec Framework Core Project</description>
	<properties>
		<java.version>11</java.version>
		<spring.cloud-version>2021.0.1</spring.cloud-version>
		<swagger.version>3.0.0</swagger.version>
		<com.enhantec.application.version>0.0.1-SNAPSHOT</com.enhantec.application.version>
		<spring-social.version>1.1.4.RELEASE</spring-social.version>
		<jjwt.version>0.11.1</jjwt.version>
		<problem.version>0.26.1</problem.version>
		<mybatisplus.version>3.5.1</mybatisplus.version>
